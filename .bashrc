## Custom
alias dev='cd ~/Documents/Dev'
alias f='find . name $1 2>/dev/null'
alias ll='ls -alrth'

## get rid of command not found ##
alias cd..='cd ..'
 
## a quick way to get out of current directory ##
alias ..='cd ..'
alias ...='cd ../../../'
alias ....='cd ../../../../'
alias .....='cd ../../../../'
alias .4='cd ../../../../'
alias .5='cd ../../../../..'
